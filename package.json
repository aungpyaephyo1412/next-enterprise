{
  "name": "next-enterprise-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "format:check": "prettier --check --ignore-path .gitignore '**/*.ts' '**/*.tsx'",
    "format:fix": "prettier --write --ignore-path .gitignore '**/*.ts' '**/*.tsx'",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "src/**/*": [
      "npm run format:fix",
      "npm run test"
    ]
  },
  "dependencies": {
    "lucide-react": "^0.401.0",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.19",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@next/bundle-analyzer": "^14.2.5",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vercel/otel": "^1.9.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-testing-library": "^6.2.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "next-compose-plugins": "^2.2.1",
    "next-themes": "^0.3.0",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.2",
    "typescript": "^5",
    "zod": "^3.23.8"
  }
}
